


<table class="table table-dark table-hover">
<thead>
<tr>
<th>Title</th>
<th>Description</th>
<th></th>
</tr>
</thead>
<% @audit_logs.each do |audit_log|%>

<%  active_customer = identify_active_customer(audit_log.cust_id) %>
<% if active_customer && active_customer.id > 0 %>
	<tr>
<td> <%= active_customer.asset.title %></td>
<td><%= active_customer.asset.description %></td>

<td><%= link_to "View Message", view_message_path(active_customer.asset.id,active_customer.cust_id) %></td>

	</tr>


<% end %>

<% end %>

</table>